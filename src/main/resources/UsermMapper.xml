<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.UsermMapper">
	<select id="selectUsermForXml" parameterType="Integer" resultType="userm">
		select id,username,age,phone,descr from userm where id=#{id}
	</select>
</mapper>